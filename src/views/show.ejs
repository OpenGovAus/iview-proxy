<html>
    <head>
        <%- include ('_headcontent'); %>
        <title>iView Proxy - <%= query.id %></title>
        <% if(notExists == false) { %>
            <meta name="progtype" content="<%- progtype %>">
            <meta name="id" content="<%- query.id %>">
            <meta name="sig" content="<%- signature %>">
            <meta name="dir" content="<%- dir %>">
            <link href="https://vjs.zencdn.net/7.11.4/video-js.css" rel="stylesheet" />
        <% } %>
    </head>
    <body>
        <%- include ('_header'); %>
        <% if(notExists == true) { %>
            <p>Show/series not found.</p>
        <% } else { %>
            <% if(progtype == 'show') {%>
                <script src="https://cdnjs.cloudflare.com/ajax/libs/videojs-contrib-hls/5.14.1/videojs-contrib-hls.js"></script>
                <script src="https://vjs.zencdn.net/7.2.3/video.js"></script>
                <div class="video-container"></div>
            <% }else { %>
                <div class="episode-list"></div>
            <% } %>
            <script src="/js/showapi.js"></script>
        <% } %>
    </body>
</html>